{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/app/components/log_in/index.jsx",
    "src/app/components/landpage/index.jsx",
    "src/app/utilities/index.js",
    "src/app/components/sign_up/index.jsx",
    "src/app/(pages)/login/page.jsx",
    "src/app/components/dashboard/index.jsx",
    "src/app/components/exams/index.jsx",
    ".next/dev/types/**/*.ts"
, "src/app/utilities/Fetch_toFile.js"  ],
  "exclude": [
    "node_modules"
  ]
}
